# AllUnited API Configuratie voor Lesrooster
# Vul deze waarden in in je .env bestand

# Kalender ICS URL voor de agenda op de homepage (Nextcloud CalDAV public link)
CALENDAR_ICS_URL=

# Base URL van de AllUnited API
ALLUNITED_API_URL=https://api.allunited.nl

# Client ID (bijv. HGV)
ALLUNITED_CLIENT_ID=

# API Key voor authenticatie
ALLUNITED_API_KEY=

# Section (bijv. HGV)
ALLUNITED_SECTION=

# Query ID voor het lesrooster (ophalen via /queries endpoint)
ALLUNITED_QUERY_ID=

# Freescout API Configuratie voor Formulieren
# Vul deze waarden in in je .env bestand

# Base URL van je Freescout installatie (bijv. https://support.jedomein.nl)
FREESCOUT_API_URL=

# API Key van Freescout (aanmaken via Settings > API in Freescout)
FREESCOUT_API_KEY=

# Mailbox IDs per formulier type
# Deze vind je in Freescout onder Settings > Mailboxes
FREESCOUT_MAILBOX_WIJZIGING=
FREESCOUT_MAILBOX_OPZEGGING=
FREESCOUT_MAILBOX_CONTACT=
FREESCOUT_MAILBOX_KAMP=
FREESCOUT_MAILBOX_KAMP_TEAMLIDEN=
FREESCOUT_MAILBOX_ICT=
FREESCOUT_MAILBOX_CK=
FREESCOUT_MAILBOX_KLEDING=
